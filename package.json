{
  "name": "catmash",
  "version": "1.0.0",
  "main": "index.js",
  "author": "<jcentaur@student.42.fr>",
  "license": "MIT",
  "scripts": {
    "dev:server": "yarn nodemon src/server/app.js",
    "dev:client": "webpack-dev-server",
    "db:create": "babel-node database/scripts/create.js",
    "db:drop": "babel-node database/scripts/drop.js",
    "db:truncate": "babel-node database/scripts/truncate.js",
    "db:reset": "yarn db:drop && yarn db:create && yarn db:migrate && yarn db:seed",
    "db:migrate": "yarn knex migrate:latest",
    "db:migrate:make": "yarn knex migrate:make",
    "db:rollback": "yarn knex migrate:rollback",
    "db:seed": "babel-node ./node_modules/.bin/knex seed:run",
    "build:client": "rm -rf public/dist && NODE_ENV=production webpack",
    "build:server": "rm -rf lib/ && cp -R src/ lib/ && NODE_ENV=production babel -d lib src",
    "build": "yarn build:client && yarn build:server",
    "heroku-postbuild": "yarn build && yarn db:migrate",
    "lint": "eslint .",
    "test": "jest --runInBand",
    "start": "NODE_ENV=production node lib/server/app.js",
    "storybook": "start-storybook -p 6006",
    "build:storybook": "build-storybook -o public/storybook"
  },
  "dependencies": {
    "@apollo/react-hooks": "^3.0.0",
    "@loadable/component": "^5.10.2",
    "apollo-boost": "^0.4.3",
    "apollo-server": "^2.14.2",
    "assets-webpack-plugin": "^3.9.10",
    "axios": "^0.19.0",
    "convict": "^5.1.0",
    "css-loader": "^3.2.0",
    "express": "^4.17.1",
    "graphql": "^14.4.2",
    "graphql-iso-date": "^3.6.1",
    "graphql-tag": "^2.10.1",
    "knex": "^0.19.1",
    "morgan": "^1.9.1",
    "mz": "^2.7.0",
    "objection": "^1.6.9",
    "pg": "^7.12.0",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-router-dom": "^5.0.1",
    "style-loader": "^1.0.0",
    "styled-components": "^4.3.2",
    "url-loader": "^2.1.0",
    "webpack": "^4.39.1",
    "webpack-cli": "^3.3.6",
    "webpack-dev-server": "^3.7.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/node": "^7.5.5",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-react": "^7.0.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^10.0.2",
    "babel-jest": "^24.8.0",
    "babel-loader": "^8.0.6",
    "babel-plugin-module-resolver": "^3.2.0",
    "babel-plugin-styled-components": "^1.10.6",
    "eslint": "^6.1.0",
    "eslint-config-airbnb": "^17.1.1",
    "eslint-config-prettier": "^6.0.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.14.3",
    "eslint-plugin-react-hooks": "^1.7.0",
    "factory-girl": "^5.0.4",
    "jest": "^24.8.0",
    "nodemon": "^1.19.1",
    "prettier": "^1.18.2"
  }
}
